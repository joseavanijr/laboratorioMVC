@model IEnumerable<AtendimentoHospitalar.Models.Cidade>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.ActionLink(" ", "Editar", new { id = item.CidadeId }, htmlAttributes: new { @class = "glyphicon glyphicon-pencil btn btn-default" })
            @Html.ActionLink(" ", "Excluir", new { id = item.CidadeId }, htmlAttributes: new { @class = "glyphicon glyphicon-trash btn btn-danger" })
        </td>
    </tr>
}

</table>
